# Copy this file to `.env` and fill in real API keys. DO NOT commit your `.env` to source control.
# Example environment variables used by the project.

# LLM provider keys (provide one or both depending on your provider)
OPENAI_API_KEY=
GEMINI_API_KEY=

# Optional defaults used by services/llm_service.py
# Supported values: openai, gemini, anthropic
DEFAULT_LLM_PROVIDER=gemini
DEFAULT_LLM_MODEL=gemini-2.5-flash

# Model and request tuning
LLM_DEFAULT_TEMPERATURE=0.7
LLM_DEFAULT_MAX_TOKENS=1024
LLM_TIMEOUT=30
LLM_CACHE_TTL=3600
